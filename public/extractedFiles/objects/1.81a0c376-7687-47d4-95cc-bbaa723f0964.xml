                <script>function formatBytes(bytes,decimals) {&#xD;
   if(bytes == 0) return '0 Bytes';&#xD;
   var k = 1024,&#xD;
       dm = decimals || 2,&#xD;
       sizes = ['Bytes', 'KB', 'MB', 'GB', 'TB', 'PB', 'EB', 'ZB', 'YB'],&#xD;
       i = Math.floor(Math.log(bytes) / Math.log(k));&#xD;
   return parseFloat((bytes / Math.pow(k, i)).toFixed(dm)) + ' ' + sizes[i];&#xD;
}&#xD;
&#xD;
function formatBytesSize(bytes,decimals) {&#xD;
   if(bytes == 0) return '0 Bytes';&#xD;
   var k = 1024,&#xD;
       dm = decimals || 2,&#xD;
       sizes = ['Bytes', 'KB', 'MB', 'GB', 'TB', 'PB', 'EB', 'ZB', 'YB'],&#xD;
       i = Math.floor(Math.log(bytes) / Math.log(k));&#xD;
   return sizes[i];&#xD;
}&#xD;
&#xD;
&#xD;
		var file = new java.io.File("/");&#xD;
        var usableSpace = file.getUsableSpace(); ///unallocated / free disk space in bytes.&#xD;
        var freeSpace = file.getFreeSpace(); //unallocated / free disk space in bytes.&#xD;
        var totalspace = file.getTotalSpace();&#xD;
        var usedSpace = totalspace - freeSpace;&#xD;
&#xD;
tw.local.dataSeries1.dataPoints = new tw.object.listOf.toolkit.SYSBPMUI.DataPoint();&#xD;
tw.local.dataSeries1.dataPoints[0] = new tw.object.toolkit.SYSBPMUI.DataPoint();&#xD;
tw.local.dataSeries1.dataPoints[0].value = formatBytes(freeSpace, 2);&#xD;
tw.local.dataSeries1.dataPoints[0].label = "Free Space " + formatBytesSize(freeSpace, 2) ;&#xD;
&#xD;
tw.local.dataSeries1.dataPoints[1] = new tw.object.toolkit.SYSBPMUI.DataPoint();&#xD;
tw.local.dataSeries1.dataPoints[1].value = formatBytes(usedSpace, 2);&#xD;
tw.local.dataSeries1.dataPoints[1].label = "Used Space " + formatBytesSize(usedSpace, 2) ;&#xD;
&#xD;
tw.local.dataseries = tw.local.dataSeries1;</script>
