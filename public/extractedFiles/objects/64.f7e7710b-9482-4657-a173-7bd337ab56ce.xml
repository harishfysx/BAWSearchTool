            <scriptBlock>var _this = this;&#xD;
&#xD;
this.getUsersList = function() {&#xD;
var processID = _this.context.options.processID.get("value");&#xD;
var snapshotID = _this.context.options.snapshotID.get("value");&#xD;
var userlist1 = _this.context.options.userList.get("value");&#xD;
for (var a=0; a &lt; userlist1.items.length; a++){&#xD;
userlist1.items.pop();&#xD;
}&#xD;
_this.context.options.userList.set("value", userlist1);&#xD;
&#xD;
var sdt = this.ui.get("ProcessStepsSingle_Select1");&#xD;
sdt.clearItems();&#xD;
//sdt.setSelectedItem("");&#xD;
&#xD;
if (processID &amp;&amp; snapshotID) {&#xD;
var xhrArgs = function(config) { return {&#xD;
url: "/rest/bpm/wle/v1/processModel/" + processID + "?snapshotId="  + snapshotID + "&amp;parts=diagram", content: {&#xD;
accept: "application/json"&#xD;
},&#xD;
load: displayUsers,&#xD;
handleAs: "json"} };&#xD;
&#xD;
try{&#xD;
&#xD;
deferred = dojo.xhrGet(xhrArgs ( { inputs: {}}));&#xD;
&#xD;
} catch (error) {&#xD;
alert(error);&#xD;
}&#xD;
}&#xD;
&#xD;
function displayUsers(json_results) {&#xD;
try{&#xD;
var userlist = _this.context.options.userList.get("value");&#xD;
for (var a=0; a &lt; userlist.items.length; a++){&#xD;
userlist.items.pop();&#xD;
}&#xD;
&#xD;
for (var i=0; i &lt; json_results.data.Diagram.step.length; i++){&#xD;
var userline = userlist.createListElement();&#xD;
userline.name = json_results.data.Diagram.step[i].name;&#xD;
userline.value = json_results.data.Diagram.step[i].ID;&#xD;
userlist.add(userline);&#xD;
}&#xD;
_this.context.options.userList.set("value", userlist);&#xD;
} catch (error) {&#xD;
alert(error);&#xD;
}&#xD;
&#xD;
}&#xD;
&#xD;
}</scriptBlock>
